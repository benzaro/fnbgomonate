service firebase.storage {
  match /b/{bucket}/o {
    match /qr-codes/{qrCodeId}/{allPaths=**} {
      allow read: if request.auth != null;
      allow write: if false; // Only backend generates QR codes
    }
    match /reports/{reportId}/{allPaths=**} {
      allow read: if request.auth != null && 
                 firestore.get(/databases/(default)/documents/users/$(request.auth.uid)).data.role == 'superadmin';
      allow write: if false; // Only backend generates reports
    }
    match /csv-imports/{importId}/{allPaths=**} {
      allow read, write: if request.auth != null && 
                          firestore.get(/databases/(default)/documents/users/$(request.auth.uid)).data.role == 'superadmin';
    }
  }
}
